{
    "name": "restaurant",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "core-js": "^3.4.4",
        "cube-ui": "~1.12.34",
        "fastclick": "^1.0.6",
        "vue": "^2.6.10",
        "vue-router": "^3.0.6",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^4.0.0",
        "@vue/cli-plugin-eslint": "^4.0.0",
        "@vue/cli-plugin-router": "^4.1.1",
        "@vue/cli-plugin-vuex": "^4.0.0",
        "@vue/cli-service": "^4.1.1",
        "@vue/eslint-config-prettier": "^5.0.0",
        "axios": "^0.18.0",
        "better-scroll": "^1.15.2",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.16.0",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-vue": "^5.0.0",
        "prettier": "^1.18.2",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "vue-cli-plugin-axios": "0.0.4",
        "vue-cli-plugin-cube-ui": "^0.2.5",
        "vue-template-compiler": "^2.6.10"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "@vue/prettier"
        ],
        "rules": {
            "no-console": "off"
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 11",
        "Android >= 4.0",
        "iOS >= 8"
    ],
    "transformModules": {
        "cube-ui": {
            "transform": "cube-ui/src/modules/${member}",
            "kebabCase": true
        }
    }
}